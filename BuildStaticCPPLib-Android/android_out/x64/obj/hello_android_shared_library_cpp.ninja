defines =
include_dirs = -I../.. -Igen
cflags = -g
cflags_cc = --std=c++17 --stdlib=libc++
label_name = hello_android_shared_library_cpp
target_out_dir = obj
target_output_name = libhello_android_shared_library_cpp

build obj/hello_android_shared_library_cpp/hello_world_impl.o: cxx ../../hello_world_impl.cpp
  source_file_part = hello_world_impl.cpp
  source_name_part = hello_world_impl

build ./libhello_android_shared_library_cpp.so: solink obj/hello_android_shared_library_cpp/hello_world_impl.o
  ldflags = --stdlib=libc++
  libs =
  frameworks =
  swiftmodules =
  output_extension = .so
  output_dir = .
